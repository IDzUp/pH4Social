/*--------------header start here--------*/
header
{
  height:auto;
  margin:0;
  padding:0;
}
.logo
{
margin:18px 0 18px 0;

}
.sign {
    background:#333333;
    border-radius: 3px;
    color: #ffffff;
    font-family: Arial,Helvetica,sans-serif;
    font-size: 14px;
    font-weight: normal;
    padding: 9px 24px;
    text-align: center;
	text-transform:uppercase;
    width: 100%;
}
.sign:hover {
    background:#00988a;
    color: #ffffff;
    transition: all 0.3s ease-out 0s;
	text-decoration:none;
}
.sign:focus {
    background: none repeat scroll 0 0 #00988a;
    transition: all 0.3s ease-out 0s;
	color: #ffffff;
	text-decoration:none;
}
.sign_box
{
 float:right;
 margin:20px 0 20px 0;
}
