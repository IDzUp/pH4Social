/*----social icon start here----*/
.social-icons {
	padding-top: 5px;
	margin: 68px 0;
	list-style-type: none!important;
	float:left;
	width:100%;
}
.social-icons li{
	float: left!important;
	margin-right: 4px!important;
	margin-bottom: 4px!important;
	padding: 0px!important;
	display: inline!important;
	width: auto!important;

}			
.social-icons li a {
	-moz-transition: all 0.3s ease 0s;
	display: block!important;
	float: left!important;
	height: 100%!important;
	margin: 0!important;
	padding: 0!important;
	width: 35px!important;
	height:35px!important;
	text-indent: -9999px!important;
}
.social-icons li a:hover {
	background-position: 0 -40px!important;
}
.social-icons li a {
	background-color:transparent;
}

.social-icons li.facebook a {
	background: url(../imagesfb/facebook.png) no-repeat;
	border-radius: 50%;
	background-color: #838181;
}
.social-icons li.facebook a:hover {
	background-color: #00988a;
}

.social-icons li.googleplus a {
	background: url(../imagesfb/googleplus.png) no-repeat ;
	border-radius: 50%;
	background-color: #838181;
}
.social-icons li.googleplus a:hover {
	background-color: #00988a;
}

.social-icons li.twitter a {
	background: url(../imagesfb/twitter.png) no-repeat;
	border-radius: 50%;
	background-color: #838181;
}
.social-icons li.twitter a:hover {
	background-color: #00988a;
}
.social-icons li.youtube a {
	background: url(../imagesfb/youtube.png) no-repeat ;
	border-radius: 50%;
	background-color: #838181;
}
.social-icons li.youtube a:hover {
	background-color: #00988a;
}
