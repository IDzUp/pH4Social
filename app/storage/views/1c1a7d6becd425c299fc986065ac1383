{
alert('Required comment section');
}
else
{
  

 $.ajax({

    type: "GET",
    url: "<?php echo asset('/'); ?>/index.php/postcomdelete/"+id,
  //  data: { id: id, user: user, comment: comment, name: name},
    dataType: 'json',
    cache: false,
    success: function(data)
    {




  var comment='#cc'+data[0].idss; 
  $(comment).hide('slow');


    }
    });
}



});


