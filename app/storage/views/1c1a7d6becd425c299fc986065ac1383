<script type="text/javascript">



$(document).ready(function()
{



 $(".fancybox").on("click", function(){
    
var id=$(this).attr("id");
var divs='#post'+id;
var left='#fleft'+id;
var right='#rleft'+id;
var img='a#'+id+' > img';
var comment='#rleft'+id+' #ppcomment'+id;
var close='.cl'+id;
var btn='button#'+id;
 //   $("#post11").css("display", "block");

  $(''+btn+'').css({
            "display":"none"
       });

 
    $(''+close+'').css({
            "display":"block"
       });

        $('body').css({
            "background":"none repeat scroll 0 0 #000000",
            
